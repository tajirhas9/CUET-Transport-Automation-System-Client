AACD;;AACD,mBAAU,QAAQ,CAAC,KAAnB;AACD,CAPD;;AASA,OAAO,IAAM,aAAa,GAAG,SAAhB,aAAgB,GAAK;AAChC,MAAI,UAAU,CAAC,KAAf,EAAsB;AACpB,QAAI,UAAU,CAAC,IAAX,KAAoB,KAAK,CAAC,SAA9B,EAAyC;AACvC,aAAO,KAAP;AACD,KAFD,MAEO;AACL,UAAI,UAAU,CAAC,IAAX,KAAoB,KAAK,CAAC,KAA9B,EAAqC;AACnC,eAAO,IAAP;AACD,OAFD,MAEO;AACL,eAAO,KAAP;AACD;AACF;AACF;;AACD,SAAO,KAAP;AACD,CAbM;AAeP,MAAM,CAAC,UAAP;AAAA,qEAAkB,iBAAM,EAAN,EAAiB,CAAjB,EAA2B,IAA3B;AAAA;AAAA;AAAA;AAAA;AAAA;AAChB;AACA,YAAA,SAAS,CAAC,KAAV,GAFgB,CAIhB;;AAJgB,iBAKZ,UAAU,CAAC,KALC;AAAA;AAAA;AAAA;;AAAA,kBAMV,EAAE,CAAC,IAAH,KAAY,QANF;AAAA;AAAA;AAAA;;AAOZ;AACA,YAAA,IAAI,CAAC;AAAE,cAAA,IAAI,EAAE;AAAR,aAAD,CAAJ;AACA,YAAA,SAAS,CAAC,IAAV;AATY;AAAA;;AAAA;AAAA,kBAYR,UAAU,CAAC,IAAX,KAAoB,KAAK,CAAC,SAA1B,IAAuC,gBAAgB,CAAC,aAAjB,CAA+B,MAA/B,KAA0C,CAZzE;AAAA;AAAA;AAAA;;AAAA;AAcR,YAAA,OAAO,CAAC,GAAR,CAAY,oBAAZ,EAAkC,UAAU,CAAC,IAA7C;AAdQ;AAAA,mBAeF,UAAU,CAAC,WAAX,EAfE;;AAAA;AAgBF,YAAA,IAhBE,GAgBK,UAAU,CAAC,IAhBhB;AAiBR,YAAA,OAAO,CAAC,GAAR,CAAY,MAAZ,EAAoB,IAApB,EAjBQ,CAkBR;;AACA,YAAA,gBAAgB,CAAC,cAAjB,CAAgC,IAAhC,EAnBQ,CAoBR;;AACA,YAAA,gBAAgB,CAAC,aAAjB,CAA+B,OAA/B,CAAuC,UAAA,KAAK,EAAG;AAC7C,cAAA,MAAM,CAAC,QAAP,CAAgB,KAAhB;AACD,aAFD,EArBQ,CAwBR;AACA;;AACA,YAAA,IAAI,iCAAM,EAAN;AAAU,cAAA,OAAO,EAAE;AAAnB,eAAJ;AA1BQ;AAAA;;AAAA;AAAA;AAAA;AA4BR;AACA,YAAA,UAAU,CAAC,UAAX;AACA,YAAA,OAAO,CAAC,KAAR,CAAc,eAAd;AACA,YAAA,IAAI,2BAAoB,EAAE,CAAC,IAAvB,EAAJ;AACA,YAAA,SAAS,CAAC,IAAV;;AAhCQ;AAAA;AAAA;;AAAA;AAmCV,YAAA,IAAI;;AAnCM;AAAA;AAAA;;AAAA;AAuCd;AACA,gBAAI,SAAS,CAAC,OAAV,CAAkB,EAAE,CAAC,IAArB,MAA+B,CAAC,CAApC,EAAuC;AACrC;AACA,cAAA,IAAI;AACL,aAHD,MAGO;AACL;AACA,cAAA,IAAI,2BAAoB,EAAE,CAAC,IAAvB,EAAJ;AACA,cAAA,SAAS,CAAC,IAAV;AACD;;AA/Ca;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GAAlB;;AAAA;AAAA;AAAA;AAAA;AAmDA,MAAM,CAAC,SAAP,CAAiB,UAAC,EAAD,EAAc;AAAA;;AAC7B;AACA;AACA,EAAA,SAAS,CAAC,IAAV,GAH6B,CAK7B;;AACA,EAAA,QAAQ,CAAC,KAAT,GAAiB,YAAY,aAAC,EAAE,CAAC,IAAJ,6CAAC,SAAS,KAAV,CAA7B;AACD,CAPD","sourcesContent":["import router from './router'\nimport NProgress from 'nprogress'\nimport 'nprogress/nprogress.css'\nimport { Message } from 'element-ui'\nimport { Route } from 'vue-router'\nimport { UserModule } from '@/store/modules/user'\nimport { PermissionModule } from '@/store/modules/permission'\nimport i18n from '@/lang' // Internationalization\nimport settings from './settings'\nimport { Roles } from './api/enums'\n\nNProgress.configure({ showSpinner: false })\n\nconst whiteList = ['/login', '/auth-redirect']\n\nconst getPageTitle = (key: string) => {\n  const hasKey = i18n.te(`route.${key}`)\n  if (hasKey) {\n    const pageName = i18n.t(`route.${key}`)\n    return `${pageName} - ${settings.title}`\n  }\n  return `${settings.title}`\n}\n\nexport const getEditAccess = () => {\n  if (UserModule.token) {\n    if (UserModule.role === Roles.undefined) {\n      return false\n    } else {\n      if (UserModule.role === Roles.admin) {\n        return true\n      } else {\n        return false\n      }\n    }\n  }\n  return false\n}\n\nrouter.beforeEach(async(to: Route, _: Route, next: any) => {\n  // Start progress bar\n  NProgress.start()\n\n  // Determine whether the user has logged in\n  if (UserModule.token) {\n    if (to.path === '/login') {\n      // If is logged in, redirect to the home page\n      next({ path: '/' })\n      NProgress.done()\n    } else {\n      // Check whether the user has obtained his permission roles\n      if (UserModule.role === Roles.undefined || PermissionModule.dynamicRoutes.length === 0) {\n        try {\n          console.log('generating routes.', UserModule.role)\n          await UserModule.GetUserInfo()\n          const role = UserModule.role\n          console.log('role', role)\n          // Generate accessible routes map based on role\n          PermissionModule.GenerateRoutes(role)\n          // Dynamically add accessible routes\n          PermissionModule.dynamicRoutes.forEach(route => {\n            router.addRoute(route)\n          })\n          // Hack: ensure addRoutes is complete\n          // Set the replace: true, so the navigation will not leave a history record\n          next({ ...to, replace: true })\n        } catch (err) {\n          // Remove token and redirect to login page\n          UserModule.ResetToken()\n          Message.error('Network Error')\n          next(`/login?redirect=${to.path}`)\n          NProgress.done()\n        }\n      } else {\n        next()\n      }\n    }\n  } else {\n    // Has no token\n    if (whiteList.indexOf(to.path) !== -1) {\n      // In the free login whitelist, go directly\n      next()\n    } else {\n      // Other pages that do not have permission to access are redirected to the login page.\n      next(`/login?redirect=${to.path}`)\n      NProgress.done()\n    }\n  }\n})\n\nrouter.afterEach((to: Route) => {\n  // Finish progress bar\n  // hack: https://github.com/PanJiaChen/vue-element-admin/pull/2939\n  NProgress.done()\n\n  // set page title\n  document.title = getPageTitle(to.meta?.title)\n})\n"],"sourceRoot":""}]}